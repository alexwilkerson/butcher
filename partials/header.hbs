<div class="header_butcher" role="banner">
{{#if @blog.logo}}<img class="logo_butcher" src="{{@blog.logo}}" alt="{{@blog.title}}">{{/if}}
<h1 class="blogtitle_butcher">
<a title="{{blog.title}}" href="{{@blog.url}}">
{{{@blog.title}}}
</a>
</h1>
{{#if @blog.description}}
  <p class="blogdescription_butcher">
  {{@blog.description}}
  </p>
{{/if}}
</div>
